        "import ipywidgets as widgets\n",
        "from IPython.display import display, Markdown\n",
        "\n",
        "# Steps to implement:\n",
        "# 1. Load models and tokenizers (GPT-2 and Qwen).\n",
        "# 2. Define a helper function to generate text with different decoding strategies.\n",
        "# 3. Create interactive UI elements (prompt box, model selector, strategy selector, temperature slider).\n",
        "# 4. Add a button to trigger text generation.\n",
        "# 5. Define the button‚Äôs behavior.\n",
        "# 6. Display the full UI for the playground.\n",
        "\n",
        "\"\"\"\n",
        "YOUR CODE HERE (~3-5 lines of code)\n",
        "\"\"\"\n",
        "import ipywidgets as widgets\n",
        "from IPython.display import display, Markdown\n",
        "\n",
        "# 1‚Äì3. UI elements\n",
        "prompt_box = widgets.Textarea(\n",
        "    value=\"Write a short story about a robot learning to paint.\",\n",
        "    description=\"Prompt:\",\n",
        "    layout=widgets.Layout(width=\"100%\", height=\"120px\")\n",
        ")\n",
        "\n",
        "model_dropdown = widgets.Dropdown(\n",
        "    options=[\"GPT-2\", \"Qwen\"],\n",
        "    value=\"GPT-2\",\n",
        "    description=\"Model:\"\n",
        ")\n",
        "\n",
        "strategy_dropdown = widgets.Dropdown(\n",
        "    options=[(\"greedy\", \"greedy\"), (\"top-k\", \"top_k\"), (\"top-p\", \"top_p\")],\n",
        "    value=\"greedy\",\n",
        "    description=\"Strategy:\"\n",
        ")\n",
        "\n",
        "temperature_slider = widgets.FloatSlider(\n",
        "    value=1.0,\n",
        "    min=0.1,\n",
        "    max=2.0,\n",
        "    step=0.1,\n",
        "    description=\"Temp:\"\n",
        ")\n",
        "\n",
        "generate_button = widgets.Button(\n",
        "    description=\"Generate\",\n",
        "    button_style=\"primary\"\n",
        ")\n",
        "\n",
        "output_area = widgets.Output()\n",
        "\n",
        "# 4‚Äì5. Helper + button behavior\n",
        "def run_generation(_):\n",
        "    with output_area:\n",
        "        output_area.clear_output()\n",
        "\n",
        "        prompt = prompt_box.value.strip()\n",
        "        if not prompt:\n",
        "            display(Markdown(\"**Please enter a prompt.**\"))\n",
        "            return\n",
        "\n",
        "        # Pick model/tokenizer\n",
        "        if model_dropdown.value == \"GPT-2\":\n",
        "            model, tokenizer = gpt2_model, gpt2_tokenizer\n",
        "        else:\n",
        "            model, tokenizer = qwen_model, qwen_tokenizer\n",
        "\n",
        "        device = next(model.parameters()).device\n",
        "        input_ids = tokenizer.encode(prompt, return_tensors=\"pt\").to(device)\n",
        "\n",
        "        strategy = strategy_dropdown.value\n",
        "        temp = float(temperature_slider.value)\n",
        "\n",
        "        gen_kwargs = {\"max_new_tokens\": 128}\n",
        "\n",
        "        if strategy == \"greedy\":\n",
        "            gen_kwargs.update({\"do_sample\": False})\n",
        "        elif strategy == \"top_k\":\n",
        "            gen_kwargs.update({\"do_sample\": True, \"top_k\": 50, \"temperature\": temp})\n",
        "        elif strategy == \"top_p\":\n",
        "            gen_kwargs.update({\"do_sample\": True, \"top_p\": 0.9, \"temperature\": temp})\n",
        "\n",
        "        with torch.no_grad():\n",
        "            generated_ids = model.generate(input_ids, **gen_kwargs)\n",
        "\n",
        "        text = tokenizer.decode(generated_ids[0], skip_special_tokens=True)\n",
        "        display(Markdown(f\"**Output:**\\n\\n{text}\"))\n",
        "\n",
        "generate_button.on_click(run_generation)\n",
        "\n",
        "# 6. Display UI\n",
        "ui = widgets.VBox([\n",
        "    prompt_box,\n",
        "    widgets.HBox([model_dropdown, strategy_dropdown, temperature_slider, generate_button]),\n",
        "    output_area\n",
        "])\n",
        "\n",
        "display(ui)\n"
      ]
