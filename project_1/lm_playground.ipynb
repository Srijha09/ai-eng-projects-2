        "import torch\n",
        "from transformers import AutoTokenizer, AutoModelForCausalLM\n",
        "\n",
        "# Load both GPT-2 and Qwen models using HuggingFace `.from_pretrained` method.\n",
        "\"\"\"\n",
        "YOUR CODE HERE (~10-15 lines of code)\n",
        "\"\"\"\n",
        "# GPT-2 (completion-only)\n",
        "gpt2_model_id = \"gpt2\"\n",
        "gpt2_tokenizer = AutoTokenizer.from_pretrained(gpt2_model_id)\n",
        "gpt2_model = AutoModelForCausalLM.from_pretrained(gpt2_model_id)\n",
        "\n",
        "# Qwen3 0.6B (instruction-tuned)\n",
        "qwen_model_id = \"Qwen/Qwen2.5-0.5B-Instruct\"\n",
        "qwen_tokenizer = AutoTokenizer.from_pretrained(qwen_model_id)\n",
        "qwen_model = AutoModelForCausalLM.from_pretrained(qwen_model_id)\n",
        "\n",
        "# Move to GPU if available\n",
        "device = \"cuda\" if torch.cuda.is_available() else \"cpu\"\n",
        "gpt2_model.to(device)\n",
        "qwen_model.to(device)\n",
        "\n",
        "print(\"Loaded GPT-2 and Qwen models successfully.\")"
      ]
    },
